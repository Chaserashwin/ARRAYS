Q:https://leetcode.com/problems/move-zeroes/
solution:
class Solution {
public:
    void moveZeroes(vector<int>& nums) {
        int n=nums.size();
        if(n==0||n==1)
            return ;
        int r=0,l=0;
        while(r<n)
        {
            if(nums[r]==0)
            r++;
            else
            {
                swap(nums[r],nums[l]);
                r++; l++;
            }
        }
    }
};

